<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.apple.CoreLocationAgent</string>
	<key>MachServices</key>
	<dict>
		<key>com.apple.CoreLocation.agent</key>
		<true/>
	</dict>
	<key>OnDemand</key>
	<true/>
	<key>ProgramArguments</key>
	<array>
		<string>/System/Library/CoreServices/CoreLocationAgent.app/Contents/MacOS/CoreLocationAgent</string>
	</array>
	<key>ThrottleInterval</key>
	<integer>1</integer>
